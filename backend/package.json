{
  "name": "pal-backend",
  "version": "1.0.0",
  "description": "P.A.L. Backend Services - AI-powered campus onboarding platform",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "vitest",
    "test:run": "vitest run",
    "test:data-layer": "tsx -r dotenv/config src/test-data-layer.ts",
    "lint": "eslint src --ext .ts",
    "db:migrate": "tsx -r dotenv/config src/db/migrate.ts",
    "db:seed": "tsx -r dotenv/config src/db/seed.ts",
    "db:setup-storage": "tsx -r dotenv/config src/db/setup-storage.ts",
    "check-env": "tsx -r dotenv/config src/check-env.ts"
  },
  "keywords": [
    "ai",
    "onboarding",
    "education",
    "rag",
    "vision-ai"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@google/generative-ai": "^0.21.0",
    "@langchain/community": "^0.3.21",
    "@langchain/core": "^1.1.26",
    "@langchain/google-genai": "^0.1.5",
    "@supabase/supabase-js": "^2.47.10",
    "@types/pg": "^8.16.0",
    "bcrypt": "^5.1.1",
    "bull": "^4.16.3",
    "chromadb": "^1.9.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.0",
    "express-validator": "^7.2.1",
    "helmet": "^8.0.0",
    "jsonwebtoken": "^9.0.2",
    "langchain": "^0.3.12",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.18.0",
    "redis": "^4.7.0",
    "winston": "^3.17.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.10.5",
    "@typescript-eslint/eslint-plugin": "^8.20.0",
    "@typescript-eslint/parser": "^8.20.0",
    "eslint": "^9.18.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "vitest": "^2.1.8"
  }
}
